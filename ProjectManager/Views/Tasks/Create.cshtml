@*@using TaskBud.Business.Services*@
@model ProjectManager.BLL.ViewModels.ProjectViewModel

@*@inject PriorityHelper PriorityHelper*@

@{
    ViewBag.Title = "Project - New";
}

<h2>@ViewBag.Title</h2>

<form asp-action="Create" method="post">
    
